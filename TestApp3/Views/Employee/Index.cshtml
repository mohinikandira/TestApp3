@model List<TestApp3.Dto.EmployeeDto>

    <table class="table-bordered table">
        <tr>

        <th>Id</th>
        <th>Name</th>
        <th>gender</th>
        <th>Department</th>
        <th>Action</th>
        
        </tr>
        @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.DepartmentName</td>
           
            <td>
                <a href="/Employee/AddEdit"@item.Id>AddEdit</a>
                <a href="/Employee/Delete" @item.Id>Delete</a>

            </td>
        </tr>
    }

    </table>
    <a href="/employee/addedit">AddRecourd</a>